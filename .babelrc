{
  "presets": [
    ["next/babel"],
    [
      "@babel/preset-env",
      {
        "targets": ["> 0.25%", "not dead", "ie >= 11", "not op_mini all"],
        "modules": "cjs"
      }
    ]
  ],
  "plugins": [
    [
      "styled-components",
      {
        "ssr": true,
        "displayName": true,
        "preprocess": false
      }
    ],
    [
      "@babel/plugin-transform-runtime",
      {
        "corejs": 3,
        "regenerator": true
      }
    ]
  ]
}
